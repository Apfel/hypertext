{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cSpell.allowCompoundWords": true,
    "cmake.buildDirectory": "${workspaceFolder}/Build",
    "cmake.configureArgs": [ "-DBUILD_TESTS=ON", "-DBUILD_SHARED=ON" ],
    "files.associations": {
        "*.rc": "c",
        "*.h": "c"
    }
}
